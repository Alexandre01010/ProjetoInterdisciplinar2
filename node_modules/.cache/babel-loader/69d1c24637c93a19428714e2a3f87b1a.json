  }}</b-badge>\r\n                    <b-badge id=\"filterInfo\" class=\"mr-1\"\r\n                      >{{ getAnswersCp.length }} resultados</b-badge\r\n                    >\r\n                  </p>\r\n                </div>\r\n              </b-col>\r\n              <b-col\r\n                id=\"filterButton\"\r\n                class=\"d-flex justify-content-end\"\r\n                md=\"3\"\r\n              >\r\n                <b-button\r\n                  id=\"filterBtn\"\r\n                  variant=\"light\"\r\n                  data-bs-toggle=\"dropdown\"\r\n                  ><b-icon icon=\"filter-right\" aria-hidden=\"true\"></b-icon\r\n                ></b-button>\r\n                <ul\r\n                  id=\"dropdownFilter\"\r\n                  class=\"dropdown-menu\"\r\n                  aria-labelledby=\"dropdownMenuButton2\"\r\n                >\r\n                  <li>\r\n                    <a class=\"dropdown-item\">Todas</a>\r\n                  </li>\r\n                </ul>\r\n              </b-col>\r\n            </b-row>\r\n          </div>\r\n          <!--Cards-->\r\n          <div class=\"mt-1\" id=\"cardsDisplay\" v-if=\"getAnswersCp.length > 0\">\r\n            <b-alert class=\"alert mt-3\" show variant=\"light\" v-for=\"answer in getAnswersCp\" :key=\"answer.texto_resposta\">\r\n              <b-row class=\"mt-2\" >\r\n                <b-col md=\"2\" >\r\n                  <b-avatar size=\"50px\" :src=\"getFoto\"></b-avatar>\r\n                </b-col>\r\n                <b-col md=6>\r\n                  {{answer.texto_resposta}}\r\n                </b-col>\r\n                <b-col md=\"4\" class=\" d-flex justify-content-end\">\r\n                  <b-icon icon=\"chat-left-fill\" style=\"width: 20px; height: 20px; color: #c74620\" />\r\n                  <b-icon-arrow-up style=\"width: 25px; height: 25px; color: #0077b6\" class=\"arrow-up-short ml-3\"/>\r\n                </b-col>\r\n              </b-row>\r\n              \r\n            </b-alert>\r\n          </div>\r\n          <div class=\"align-self-center mt-3\" v-else>\r\n            <warning\r\n              message=\"NÃ£o foram encontrados resultados para a pesquisa!\"\r\n            />\r\n          </div>\r\n        </b-col>\r\n      </div>\r\n    </b-col>\r\n  </div>\r\n</template>\r\n<script>\r\nimport warning from \"../components/warning.vue\";\r\n//import cardEstagio from '../components/cardEstagio.vue'\r\nexport default {\r\n  name: \"MinhasPropostas.vue\",\r\n  components: {\r\n    warning,\r\n  },\r\n  props: {\r\n    tema: Object,\r\n    forum: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      selectedOption: \"all\",\r\n      filterText: \"\",\r\n      search: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async getAuthorFoto() {\r\n      try {\r\n        await this.$store.dispatch(\"fetchUserById\",this.forum.id_user);\r\n      } catch (error) {\r\n        console.log(error);\r\n        this.content =\r\n          (error.response && error.response.data) ||  error.message || error.toString();\r\n      } finally {\r\n        // calls getter getMessage and result is put inside content component data\r\n        this.content = this.getMessage;\r\n      }\r\n    },\r\n    async getAnswers() {\r\n      try {\r\n        await this.$store.dispatch(\"fetchAnswers\", this.tema);\r\n        \r\n      } catch (error) {\r\n        console.log(error);\r\n        this.content =\r\n          (error.response && error.response.data) ||\r\n          error.message ||\r\n          error.toString();\r\n      } finally {\r\n        // calls getter getMessage and result is put inside content component data\r\n        this.content = this.getMessage;\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    getAnswersCp() {\r\n      console.log(this.$store.getters.getFilterdAnswers())\r\n      return this.$store.getters.getFilterdAnswers(this.search);\r\n    },\r\n  },\r\n  created() {\r\n    this.getAnswers();\r\n  },\r\n};\r\n</script>\r\n