file":"Entrevistas.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"page\">\r\n    <b-col md=\"12\">\r\n      <b-col md=\"12\">\r\n        <p id=\"title\" class=\"mb-5\">Minhas Entrevistas</p>\r\n      </b-col>\r\n      <b-col md=\"12\">\r\n        <b-row>\r\n          <b-col cols=\"8\" class=\"mb-3\">\r\n            <label class=\"position-relative d-block input\">\r\n              <b-icon\r\n                icon=\"search\"\r\n                class=\"position-absolute h-100 text-muted ml-2\"\r\n              ></b-icon>\r\n              <b-input\r\n                style=\"padding-left: 30px\"\r\n                v-model=\"search\"\r\n                placeholder=\"Search\"\r\n                type=\"text\"\r\n                class=\"input\"\r\n              ></b-input>\r\n            </label>\r\n          </b-col>\r\n          <b-col md=\"2\">\r\n            <b-form-select v-model=\"cargo\" :options=\"cargos\"></b-form-select>\r\n          </b-col>\r\n          <b-col md=\"2\">\r\n            <b-button\r\n              id=\"agendar\"\r\n              type=\"submit\"\r\n              @click=\"$bvModal.show('agendar_modal')\"\r\n              >Agendar Entrevista</b-button\r\n            >\r\n          </b-col>\r\n        </b-row>\r\n        <b-row>\r\n          <b-col v-for=\"interview in interviews\" :key=\"interview.id\" cols=\"12\">\r\n            <b-card class=\"mt-5 interviewCard\">\r\n              <b-col md=\"3\">\r\n                <p class=\"cardIdentification\">\r\n                  <b-icon\r\n                    style=\"color: #0077b6\"\r\n                    icon=\"mic-fill\"\r\n                    aria-hidden=\"true\"\r\n                  ></b-icon>\r\n                  Entrevista\r\n                </p>\r\n              </b-col>\r\n              <b-col md=\"12\">\r\n                <b-row>\r\n                  <b-col class=\"fw-bolder\" md=\"8\">\r\n                    <div class=\"title\">{{ interview.title }}</div>\r\n                    <b-col class=\"text-muted\" md=\"12\">\r\n                      <p class=\"participants\">Data: {{ interview.date }}</p>\r\n                    </b-col>\r\n                    <b-col class=\"text-muted\" md=\"12\">\r\n                      <p class=\"participants\">Hora: {{ interview.time }}</p>\r\n                    </b-col>\r\n                    <b-col class=\"text-muted\" md=\"12\">\r\n                      <p class=\"participants\">\r\n                        Nº participantes: {{ interview.participant }}\r\n                      </p>\r\n                    </b-col>\r\n                  </b-col>\r\n                  <b-col md=\"4\">\r\n                    <div class=\"d-flex justify-content-end\">\r\n                      <b-button class=\"btnOpenInt\" href=\"#\" variant=\"primary\"\r\n                        >Opções</b-button\r\n                      >\r\n                    </div>\r\n                  </b-col>\r\n                </b-row>\r\n              </b-col>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-col>\r\n\r\n      <div>\r\n        <b-modal id=\"agendar_modal\" size=\"lg\" hide-footer hide-header>\r\n          <div class=\"d-flex justify-content-center\">\r\n            <b-col md=\"8\">\r\n              <p id=\"title\" class=\"mb-5\">Agendar Entrevista</p>\r\n              <b-form-group\r\n                id=\"input-group-3\"\r\n                label=\"Tópico da entrevista\"\r\n                label-for=\"input-3\"\r\n                class=\"mb-4 label\"\r\n              >\r\n                <b-form-input\r\n                  id=\"input-3\"\r\n                  class=\"input\"\r\n                  v-model=\"form.topico\"\r\n                  type=\"text\"\r\n                  required\r\n                ></b-form-input>\r\n              </b-form-group>\r\n\r\n              <b-input-group>\r\n                <b-form-input\r\n                  v-model=\"form.utilizador\"\r\n                  list=\"my-list-id\"\r\n                  placeholder=\"Selecione Utilizador\"\r\n                  class=\"input\"\r\n                ></b-form-input>\r\n                <b-input-group-append>\r\n                  <b-button\r\n                    @click=\"selectedUser()\"\r\n                    class=\"input\"\r\n                    variant=\"light\"\r\n                    ><b-icon-plus id=\"plus\"\r\n                  /></b-button>\r\n                </b-input-group-append>\r\n              </b-input-group>\r\n              <datalist id=\"my-list-id\">\r\n                <option v-for=\"user in users\" :key=\"user.name\">\r\n                  {{ user.name }}\r\n                </option>\r\n              </datalist>\r\n\r\n              <p id=\"SelectedUsers\" class=\"mt-4\">Users Selecionados:</p>\r\n              <b-col\r\n                v-for=\"(user1, index) in selectedUsersArr\"\r\n                :key=\"user1.name\"\r\n              >\r\n                <div class=\"userSel\" @click=\"deleteSel(index)\">\r\n                  {{ user1.name }}\r\n                </div>\r\n              </b-col>\r\n\r\n              <b-form-datepicker\r\n                id=\"date\"\r\n                class=\"input mt-4\"\r\n                placeholder=\"Data Entrevista\"\r\n                v-model=\"form.data\"\r\n              ></b-form-datepicker>\r\n              <b-form-timepicker\r\n                class=\"input mt-4\"\r\n                v-model=\"form.hora\"\r\n              ></b-form-timepicker>\r\n              <div class=\"d-flex justify-content-center\">\r\n                <b-button\r\n                  id=\"agendar\"\r\n                  @click=\"criarAgenda()\"\r\n                  class=\"mt-4\"\r\n                  type=\"submit\"\r\n                  >Agendar</b-button\r\n                >\r\n              </div>\r\n            </b-col>\r\n          </div>\r\n        </b-modal>\r\n      </div>\r\n    </b-col>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n\r\n      selectedUsersArr: [],\r\n      form: {\r\n        topico: \"\",\r\n        utilizador: \"\",\r\n        data: this.now(),\r\n        hora: \"00:00:00\",\r\n      },\r\n      cargo: null,\r\n      cargos: [\r\n        { value: null, text: \"Cargos\" },\r\n        { value: 1, text: \"Docente\" },\r\n        { value: 2, text: \"Professor\" },\r\n        { value: 3, text: \"CCA\" },\r\n        { value: 4, text: \"Entidade Externa\" },\r\n      ],\r\n      users: this.$store.getters.getUsers,\r\n      interviews: [\r\n        {\r\n          id: 1,\r\n          date: \"13/02/2021\",\r\n          title: \"Entrevista sobre o projeto de estágio\",\r\n          participant: 6,\r\n          time: \"10:55\",\r\n        },\r\n        {\r\n          id: 2,\r\n          date: \"15/02/2030\",\r\n          title: \"Entrevista Entrega Final\",\r\n          participant: 4,\r\n          time: \"22:55\",\r\n        },\r\n        {\r\n          id: 3,\r\n          date: \"15/02/2030\",\r\n          title: \"Entrevista Entrega Final\",\r\n          participant: 4,\r\n          time: \"13:55\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    criarAgenda() {},\r\n    now() {\r\n      let today = new Date().toISOString().slice(0, 10);\r\n      return today;\r\n    },\r\n    deleteSel(index) {\r\n      this.selectedUsersArr = this.selectedUsersArr.filter(\r\n        (user) => user.name != this.selectedUsersArr[index].name\r\n      );\r\n    },\r\n    selectedUser() {\r\n      console.log(\r\n        this.users.filter((user) => user.name == this.form.utilizador)\r\n      );\r\n      if (this.users.filter((user) => user.name == this.form.utilizador)[0]) {\r\n        let a = {\r\n          value: this.users.filter(\r\n            (user) => user.name == this.form.utilizador\r\n          )[0].name,\r\n          name: this.users.filter(\r\n            (user) => user.name == this.form.utilizador\r\n          )[0].name,\r\n        };\r\n        this.selectedUsersArr.push(a);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style scoped >\r\n.userSel {\r\n  color: #707070;\r\n}\r\n.userSel:hover {\r\n  background-color: #efefef;\r\n}\r\n#plus {\r\n  color: #000;\r\n}\r\n#SelectedUsers {\r\n  color: #707070;\r\n  display: inline-block;\r\n  font-size: 5;\r\n  font-weight: lighter;\r\n  border-bottom: 1px solid #c94514;\r\n}\r\n.label {\r\n  color: #707070;\r\n  text-align: left;\r\n}\r\n.title {\r\n  font-weight: 500;\r\n}\r\n.participants {\r\n  margin: 0;\r\n}\r\n.btnOpenInt {\r\n  background-color: #0077b6;\r\n  color: #fff;\r\n  border: none;\r\n  border-radius: 19px;\r\n  width: 159px;\r\n  font-size: 14px;\r\n}\r\n.interviewCard {\r\n  background-color: #fff;\r\n  font-size: 17px;\r\n  font-family: Rubik, sans-serif;\r\n  border: none;\r\n  border-radius: 36px;\r\n  box-shadow: 2px 3px #d9d9d9;\r\n}\r\n.page {\r\n  background-color: #f5f5f5;\r\n}\r\n.input {\r\n  border-radius: 15px;\r\n  box-shadow: 2px 2px 2px 2px #e6e6e6;\r\n  border: none;\r\n}\r\n#agendar {\r\n  background-color: #0077b6;\r\n  width: 100%;\r\n  height: 40px;\r\n  border-radius: 18px;\r\n  font-weight: lighter;\r\n  font-size: 17px;\r\n  border: none;\r\n}\r\n</style>"]}]}